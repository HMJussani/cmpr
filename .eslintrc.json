{
  "parserOptions": {
    "ecmaVersion": 9,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    },
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "node": true
  },
  "plugins": ["import", "node", "promise", "standard"],
  "globals": {
    "document": false,
    "navigator": false,
    "window": false
  },
  "extends": "standard"
}
